title }}\n                </h2>\n\n                <!-- Text -->\n                <p class=\"font-size-sm mb-4\">\n                  {{ post.summary }}\n                </p>\n\n                <!-- Footer -->\n                <div class=\"d-flex align-items-center\">\n\n                  <!-- Avatar -->\n                  <div class=\"avatar avatar-xs\">\n                    <img class=\"avatar-img rounded-circle\" src=\"https://lh3.googleusercontent.com/-mGV3LBq4iCY/AAAAAAAAAAI/AAAAAAAAAAA/AMZuucmHTM2A6QKZBfGrx4BR-4ATw-OUxw/s96-c/photo.jpg\" alt=\"...\">\n                  </div>\n\n                  <!-- Body -->\n                  <div class=\"ml-4 text-left\">\n\n                    <!-- Title -->\n                    <p class=\"small mb-0\">\n                     {{ post.author.last_name }}\n                    </p>\n\n                    <!-- Position -->\n                    <p class=\"small text-muted mt-n1 mb-0\">\n                      {{ new Date(post.published).toISOString().slice(0, 10) }}\n                    </p>\n\n                  </div>\n\n                </div>\n\n                <!-- Link -->\n              </div>\n            </div>\n            </router-link>\n          </div>\n        </div>\n      </div>\n    </section>\n</template>\n\n<script>\nimport butter from '../../plugins/buttercms'\n\nexport default {\n  name: 'Welcome',\n  props: {\n\n  },\n  data: function () {\n    return {\n      posts: []\n    }\n  },\n  methods: {\n    getPosts () {\n      butter.post.list({\n        page: 1,\n        page_size: 10\n      }).then(res => {\n        this.posts = res.data.data\n      })\n    }\n  },\n  filters: {\n    img: function (value) {\n      return require('@/assets/img/' + value)\n    }\n  },\n  mounted: function () {\n    this.getPosts()\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n</style>\n"]}]}